<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
  "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
  <!-- Record constructor parameters always conflict. -->
  <suppress files="CMKeyStoreDescription\.java"
            checks="HiddenField"/>

  <!-- String from bytes. -->
  <suppress files="CMKeyStoreProvider\.java"
            checks="IllegalInstantiation"/>
  
  <!-- Not our fault that the security API uses so many checked exceptions. -->
  <suppress files="CMKeyStores\.java"
            checks="ThrowsCount"/>

</suppressions>
